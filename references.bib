@article{bordoni_anatomic_2013,
	title = {Anatomic connections of the diaphragm influence of respiration on the body system},
	issn = {1178-2390},
	url = {http://www.dovepress.com/anatomic-connections-of-the-diaphragm-influence-of-respiration-on-the--peer-reviewed-article-JMDH},
