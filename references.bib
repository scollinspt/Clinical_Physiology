@article{ruaro_low-level_2014,
	title = {Low-level laser therapy to treat fibromyalgia},
	volume = {29},
	issn = {0268-8921, 1435-604X},
	url = {http://link.springer.com/10.1007/s10103-014-1566-8},
	doi = {10.1007/s10103-014-1566-8},
	abstract = {Several clinical treatments have been proposed to manage symptoms of fibromyalgia. Low-level laser therapy (LLLT) may be a useful tool to treat this dysfunction. The aim of this study was to evaluate the effects of LLLT in patients with fibromyalgia. A placebo-controlled, randomized clinical trial was carried out with 20 patients divided randomly into either an LLLT group (n=10) or a placebo group (n=10). The LLLT group was treated with a GaAlAs laser (670 nm, 4 J/ cm2 on 18 tender points) three times a week over 4 weeks. Before and after treatment, patients were evaluated with the Fibromyalgia Impact Questionnaire (FIQ), McGill Pain Questionnaire, and visual analog scale (VAS). Data from the FIQ and McGill questionnaire for the treated and control groups were analyzed by paired t tests, and Wilcoxon tests were used to analyze data from the VAS. After LLLT or sham treatment, the number of tender points was significantly reduced in both groups (LLLT, p{\textless}0.0001; placebo, p=0.0001). However, all other fibromyalgia symptoms showed significant improvements after LLLT compared to placebo (FIQ, p=0.0003; McGill, p=0.0078; and VAS, p=0.0020). LLLT provided relief from fibromyalgia symptoms in patients and should be further investigated as a therapeutic tool for management in fibromyalgia.},
	language = {en},
	number = {6},
	urldate = {2022-03-30},
	journal = {Lasers in Medical Science},
	author = {Ruaro, J. A. and Fréz, A. R. and Ruaro, M. B. and Nicolau, R. A.},
	month = nov,
	year = {2014},
	pages = {1815--1819},
}

@article{goh_quantum_2020,
	title = {Quantum {Biology}: {Does} quantum physics hold the key to revolutionizing medicine?},
	volume = {3},
	issn = {2710-6039},
	shorttitle = {Quantum {Biology}},
	url = {https://journals.hh-publisher.com/index.php/pddbs/article/view/349},
	doi = {10.36877/pddbs.a0000130},
	abstract = {Quantum biology is the study of quantum effects on biochemical mechanisms and biological function. Quantum physics describes the behaviour of the nanoscale particles that make up all matter including living organisms, but it was generally believed that the ‘warm, wet and noisy environment’ in living organisms would result in decoherence making it impossible for meaningful quantum effects to occur. However, there is now a substantial body of work supporting that nature has actually adapted in such a way as to exploit quantum properties to enhance cellular functioning and is believed to have role in a diverse range of key processes in living organisms ranging from maintaining the stability of DNA to neuron function to conscious cognition; from light harvesting in photosynthesis to avian magnetic field based navigation. This review aims to summarize the various mechanisms and functions in living organisms believed to utilize quantum mechanics to purposefully and effectively enhance their performance, and to explore the potential this could hold in diagnosing and treating various medical conditions.},
	language = {en},
	number = {1},
	urldate = {2022-03-30},
	journal = {Progress in Drug Discovery \& Biomedical Science},
	author = {Goh, Bey Hing and Tong, Eng Siang and Pusparajah, Priyia},
	month = nov,
	year = {2020},
}

