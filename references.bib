@article{pattaranit_mathematical_2008,
	title = {Mathematical models of energy homeostasis},
	volume = {5},
	issn = {1742-5689, 1742-5662},
	url = {https://royalsocietypublishing.org/doi/10.1098/rsif.2008.0216},
	doi = {10.1098/rsif.2008.0216},
	abstract = {Diabetes and obesity present a mounting global challenge. Clinicians are increasingly turning to mechanism-based mathematical models for a quantitative definition of physiological defects such as insulin resistance, glucose intolerance and elevated obesity set points, and for predictions of the likely outcomes of therapeutic interventions. However, a very large range of such models is available, making a judicious choice difficult. To better inform this choice, here we present the most important models published to date in a uniform format, discussing similarities and differences in terms of the decisions faced by modellers. We review models for glucostasis, based on the glucose–insulin feedback control loop, and consider extensions to long-term energy balance, dislipidaemia and obesity.},
	language = {en},
	number = {27},
	urldate = {2020-12-10},
	journal = {Journal of The Royal Society Interface},
	author = {Pattaranit, Ratchada and van den Berg, Hugo Antonius},
	month = oct,
	year = {2008},
	pages = {1119--1135},
}

@article{mcewen_physiology_2007,
	title = {Physiology and {Neurobiology} of {Stress} and {Adaptation}: {Central} {Role} of the {Brain}},
	volume = {87},
	issn = {0031-9333, 1522-1210},
	shorttitle = {Physiology and {Neurobiology} of {Stress} and {Adaptation}},
	url = {https://www.physiology.org/doi/10.1152/physrev.00041.2006},
	doi = {10.1152/physrev.00041.2006},
	abstract = {The brain is the key organ of the response to stress because it determines what is threatening and, therefore, potentially stressful, as well as the physiological and behavioral responses which can be either adaptive or damaging. Stress involves two-way communication between the brain and the cardiovascular, immune, and other systems via neural and endocrine mechanisms. Beyond the “flight-or-fight” response to acute stress, there are events in daily life that produce a type of chronic stress and lead over time to wear and tear on the body (“allostatic load”). Yet, hormones associated with stress protect the body in the short-run and promote adaptation (“allostasis”). The brain is a target of stress, and the hippocampus was the first brain region, besides the hypothalamus, to be recognized as a target of glucocorticoids. Stress and stress hormones produce both adaptive and maladaptive effects on this brain region throughout the life course. Early life events influence life-long patterns of emotionality and stress responsiveness and alter the rate of brain and body aging. The hippocampus, amygdala, and prefrontal cortex undergo stress-induced structural remodeling, which alters behavioral and physiological responses. As an adjunct to pharmaceutical therapy, social and behavioral interventions such as regular physical activity and social support reduce the chronic stress burden and benefit brain and body health and resilience.},
	language = {en},
	number = {3},
	urldate = {2020-12-10},
	journal = {Physiological Reviews},
	author = {McEwen, Bruce S.},
	month = jul,
	year = {2007},
	pages = {873--904},
}

@article{gorban_correlations_2010,
	title = {Correlations, {Risk} and {Crisis}: {From} {Physiology} to {Finance}},
	volume = {389},
	issn = {03784371},
	shorttitle = {Correlations, {Risk} and {Crisis}},
	url = {http://arxiv.org/abs/0905.0129},
	doi = {10.1016/j.physa.2010.03.035},
	abstract = {We study the dynamics of correlation and variance in systems under the load of environmental factors. A universal eﬀect in ensembles of similar systems under the load of similar factors is described: in crisis, typically, even before obvious symptoms of crisis appear, correlation increases, and, at the same time, variance (and volatility) increases too. This eﬀect is supported by many experiments and observations of groups of humans, mice, trees, grassy plants, and on ﬁnancial time series.},
	language = {en},
	number = {16},
	urldate = {2020-12-10},
	journal = {Physica A: Statistical Mechanics and its Applications},
	author = {Gorban, A. N. and Smirnova, E. V. and Tyukina, T. A.},
	month = aug,
	year = {2010},
	note = {arXiv: 0905.0129},
	keywords = {Physics - Biological Physics, Physics - Physics and Society, Quantitative Finance - General Finance},
	pages = {3193--3217},
}

@article{gorban_law_2011,
	title = {Law of the {Minimum} {Paradoxes}},
	volume = {73},
	issn = {0092-8240, 1522-9602},
	url = {http://arxiv.org/abs/0907.1965},
	doi = {10.1007/s11538-010-9597-1},
	abstract = {T he “Law of the Minimum” states that growth is controlled by the scarcest resource (limiting factor). This concept was originally applied to plant or crop growth (Justus von Liebig, 1840 [1]) and quantitatively supported by many experiments. Some generalizations based on more complicated “doseresponse” curves were proposed. Violations of this law in natural and experimental ecosystems were also reported. We study models of adaptation in ensembles of similar organisms under load of environmental factors and prove that violation of Liebig’s law follows from adaptation eﬀects. If the ﬁtness of an organism in a ﬁxed environment satisﬁes the Law of the Minimum then adaptation equalizes the pressure of essential factors and therefore acts against the Liebig’s law. This is the the Law of the Minimum paradox: if for a randomly chosen pair “organism–environment” the Law of the Minimum typically holds, then, in a well-adapted system, we have to expect violations of this law.},
	language = {en},
	number = {9},
	urldate = {2020-12-10},
	journal = {Bulletin of Mathematical Biology},
	author = {Gorban, A. N. and Pokidysheva, L. I. and Smirnova, E. V. and Tyukina, T. A.},
	month = sep,
	year = {2011},
	note = {arXiv: 0907.1965},
	keywords = {Quantitative Biology - Populations and Evolution},
	pages = {2013--2044},
}

